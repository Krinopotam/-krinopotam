{
  "name": "@krinopotam/ui-kit",
  "version": "2.1.304",
  "description": "UI kit for Krinopotam's projects",
  "private": false,
  "author": "Maxim Zaytsev",
  "license": "ISC",
  "main": "index.js",
  "homepage": "https://krinopotam.github.io/-krinopotam/",
  "type": "module",
  "scripts": {
    "build_old": "tsc -p tsconfig.build.json && tsc-alias",
    "build": "tsc -p tsconfig.build.json && vite build --mode build-lib",
    "demo": "vite --mode demo",
    "build-demo-generator": "tsc -p ./demo/_generator/src/tsconfig.json",
    "build-demo": "node ./demo/_generator/build/generateDemo.js && tsc --noEmit && tsc-alias && vite build --mode demo",
    "preview-demo": "vite preview",
    "storybook": "storybook dev -p 6007",
    "build-storybook": "npx tsx ./stories/injectSources.ts && storybook build --output-dir deploy/ui-kit/storybook",
    "deploy": "gh-pages --nojekyll -d deploy",
    "analyze": "vite-bundle-visualizer",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/js": "^9.20.0",
    "@storybook/addon-console": "^3.0.0",
    "@storybook/addon-docs": "^8.5.4",
    "@storybook/addon-essentials": "^8.5.4",
    "@storybook/addon-interactions": "^8.5.4",
    "@storybook/addon-links": "^8.5.4",
    "@storybook/addon-storysource": "^8.5.4",
    "@storybook/blocks": "^8.5.4",
    "@storybook/react": "^8.5.4",
    "@storybook/react-vite": "^8.5.4",
    "@storybook/test": "^8.5.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/glob": "^8.1.0",
    "@types/jsdom": "^21.1.7",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^22.13.1",
    "@types/quill": "^2.0.14",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/react-transition-group": "^4.4.12",
    "@types/tabulator-tables": "^6.2.3",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.5",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-storybook": "^0.11.2",
    "gh-pages": "^6.3.0",
    "glob": "^11.0.1",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.0",
    "react-syntax-highlighter": "^15.6.1",
    "rollup-plugin-node-externals": "^8.0.0",
    "sass": "^1.84.0",
    "storybook": "^8.5.4",
    "terser": "^5.39.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "vite": "^6.1.0",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-lib-inject-css": "^2.2.1",
    "vite-plugin-watch": "^0.3.1",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "@krinopotam/common-hooks": "^1.1.2",
    "@krinopotam/js-helpers": "^1.0.76",
    "@krinopotam/service-types": "^1.0.21",
    "@monaco-editor/react": "^4.7.0-rc.0",
    "ahooks": "^3.8.4",
    "antd": "^5.24.0",
    "lodash.debounce": "^4.0.8",
    "monaco-editor": "^0.52.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-draggable": "^4.4.6",
    "react-quill-new": "^3.3.3",
    "react-router-dom": "^7.1.5",
    "react-sticky-box": "^2.0.5",
    "react-transition-group": "^4.4.5",
    "styled-components": "^6.1.15",
    "tabulator-tables": "^6.3.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
  },
  "overrides": {
    "ahooks": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
