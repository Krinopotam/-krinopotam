{
  "name": "@krinopotam/ui-kit",
  "version": "2.0.218",
  "description": "UI kit for Krinopotam's projects",
  "private": false,
  "author": "Maxim Zaytsev",
  "license": "ISC",
  "main": "index.js",
  "homepage": "https://krinopotam.github.io/-krinopotam/",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias",
    "demo": "vite --mode demo",
    "build-demo-generator": "tsc -p ./demo/_generator/src/tsconfig.json",
    "build-demo": "node ./demo/_generator/generateDemo.js && tsc --noEmit && tsc-alias && vite build --mode demo",
    "preview-demo": "vite preview",
    "storybook": "storybook dev -p 6007",
    "build-storybook": "npx tsx ./stories/injectSources.ts && storybook build --output-dir deploy/ui-kit/storybook",
    "deploy": "gh-pages --nojekyll -d deploy",
    "analyze": "vite-bundle-visualizer",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^2.0.2",
    "@eslint/js": "^9.10.0",
    "@storybook/addon-console": "^3.0.0",
    "@storybook/addon-docs": "8.3.0",
    "@storybook/addon-essentials": "8.3.0",
    "@storybook/addon-interactions": "8.3.0",
    "@storybook/addon-links": "8.3.0",
    "@storybook/addon-storysource": "8.3.0",
    "@storybook/blocks": "8.3.0",
    "@storybook/react": "8.3.0",
    "@storybook/react-vite": "8.3.0",
    "@storybook/test": "8.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^20.16.5",
    "@types/quill": "^2.0.14",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/tabulator-tables": "^6.2.3",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "eslint": "8.57",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "gh-pages": "^6.1.1",
    "globals": "^15.9.0",
    "jsdom": "^25.0.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "sass": "^1.78.0",
    "storybook": "8.3.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.5.0",
    "vite": "^5.4.5",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-watch": "^0.3.1",
    "vitest": "^2.1.1"
  },
  "dependencies": {
    "@krinopotam/common-hooks": "^1.0.25",
    "@krinopotam/js-helpers": "^1.0.56",
    "@krinopotam/service-types": "^1.0.19",
    "ahooks": "^3.8.1",
    "antd": "^5.20.6",
    "lodash.debounce": "^4.0.8",
    "rc-animate": "^3.1.1",
    "react-draggable": "^4.4.6",
    "react-quill": "^2.0.0",
    "react-sticky-box": "^2.0.5",
    "react-syntax-highlighter": "^15.5.0",
    "styled-components": "^6.1.13",
    "tabulator-tables": "^6.2.5",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
  }
}
