{
  "extends": "./tsconfig.json",
  "compilerOptions": {
	"types": ["node"],
	"outDir": "./demo/.generator/build",
    "rootDir": "./demo/.generator/src",
    "noImplicitAny": false,
    "declaration": false,

    //"skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ESNext",
    "isolatedModules": false,
    "esModuleInterop": false,
    "lib": [ "es2021" ]
  },
  "include": [
    "./demo/.generator/src/**/*.ts"
  ],
  "exclude": []
}